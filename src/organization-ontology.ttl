@prefix organization: <https://bibsysdev.github.io/src/organization-ontology.ttl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

organization:hasPart a rdf:Property ;
  rdfs:label "hasPart" ;
  rdfs:comment "A sub-part of an organization" ;
  rdfs:isDefinedBy organization: ;
  rdfs:domain organization:Organization ;
  rdfs:range organization:Organization .

organization:partOf a rdf:Property ;
  rdfs:label "partOf" ;
  rdfs:comment "A super-part of an organization" ;
  rdfs:isDefinedBy organization: ;
  rdfs:domain organization:Organization ;
  rdfs:range organization:Organization .

